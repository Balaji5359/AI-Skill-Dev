{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "CreateLogGroup",
			"Effect": "Allow",
			"Action": "logs:CreateLogGroup",
			"Resource": "arn:aws:logs:ap-south-1:433448338709:*"
		},
		{
			"Sid": "CreateLogStreamAndPutLogEvents",
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": [
				"arn:aws:logs:ap-south-1:433448338709:log-group:/aws/lambda/jam-topics-generator-by917:*",
				"arn:aws:logs:ap-south-1:433448338709:log-group:/aws/lambda/getPronunciationSentencesFunction:*"
			]
		}
	]
}